ll,//广告详情ID\n    orderID:window.sessionStorage.getItem('orderID') || null,//订单ID\n\n};\nconst mutations = {\n    upAmountWay: (state, opt) => {\n        state.amount_way = opt;\n        window.sessionStorage.setItem('amount_way', opt)\n    },\n    upWayRate: (state, opt) => {\n        state.way_rate = opt;\n        window.sessionStorage.setItem('way_rate', opt)\n    },\n    upLanguage: (state, opt) => {\n        state.language = opt;\n        window.localStorage.setItem('opt');\n    },\n    upAccount: (state, opt) => {\n        state.account = opt;\n        window.sessionStorage.setItem('account', JSON.stringify(opt));\n    },\n    upSymbol: (state, opt) => {\n        state.way_symbol = opt;\n        window.sessionStorage.setItem('way_symbol', opt);\n    },\n    upCoin:(state,opt) => {\n        state.coin = opt;\n        window.sessionStorage.setItem('coin',JSON.stringify(opt));\n    },\n    upChainMsg:(state,opt) => {\n        state.chantMsg = opt;\n        window.sessionStorage.setItem('chantMsg',JSON.stringify(opt));\n    },\n    upAdvID:(state,opt) => {\n        state.advID = opt;\n        window.sessionStorage.setItem('advID',opt);\n    },\n    upOrderID:(state,opt) => {\n        state.orderID = opt;\n        window.sessionStorage.setItem('orderID',opt);\n    }\n}\n\nconst actions = {\n    upUserInfo: async ({ commit }) => {\n        const token = document.cookie.match(`[;\\s+]?Admin-Token=([^;]*)`)?.pop();\n        const result = await UserInfo(token);\n        commit('upAccount', result.data);\n    }\n};\n\n\nexport default {\n    namespaced: true,\n    state,\n    mutations,\n    actions\n}"],"mappings":"AAAA,SAASA,QAAT,QAAyB,eAAzB;AACA,MAAMC,KAAK,GAAG;EACVC,UAAU,EAAEC,MAAM,CAACC,cAAP,CAAsBC,OAAtB,CAA8B,YAA9B,KAA+C,KADjD;EACuD;EACjEC,UAAU,EAAEH,MAAM,CAACC,cAAP,CAAsBC,OAAtB,CAA8B,YAA9B,KAA+C,GAFjD;EAGVE,QAAQ,EAAEJ,MAAM,CAACC,cAAP,CAAsBC,OAAtB,CAA8B,UAA9B,KAA6C,MAH7C;EAGoD;EAC9DG,QAAQ,EAAEL,MAAM,CAACM,YAAP,CAAoBJ,OAApB,CAA4B,QAA5B,KAAyC,IAJzC;EAKVK,OAAO,EAAEC,