searchFormWidth() {\n      let item = this.$store.state.public.window;\n      item.width = window.innerWidth;\n      item.height = window.innerHeight;\n    },\n  },\n  destroyed() {\n    // window.onresize = null;\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n#app {\n  width: 100%;\n  height: 100%;\n}\n</style>"]}]}