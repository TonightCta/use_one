/type == 0 && number:*,coinname:* } => { //type == 1 && coinname:number }\n    */\n    let price = [];\n    for (let i in _data) {\n        price.push(\n            await computedPrice(\n                _rate,\n                _type == 0 ? _data[i].available : _data[i],\n                _type == 0 ? _data[i].coin : i\n            )\n        );\n    };\n    return price;\n};\n//计算单个币种持有价格\nexport async function oneInterPrice(_rate,_number, _coin) {\n    let price = '';\n    price = await computedPrice(\n        _rate,\n        _number,\n        _coin\n    )\n    return price;\n}\n//获取币种行情价格\nexport async function getCoinPrice(_icon) {\n    const result = await api.QU();\n    const { data } = result;\n    const { list } = data;\n    return list[_icon].price;\n}"],"mappings":";;;;;;;;;;;;;;AAAA;;AACA;AACO,SAASA,UAAT,CAAoBC,GAApB,EAAyB;EAC5B,MAAMC,IAAI,GAAG,oDAAb;EACA,OAAOA,IAAI,CAACC,IAAL,CAAUF,GAAV,CAAP;AACH,C,CACD;;;AACO,SAASG,aAAT,CAAuBH,GAAvB,EAA4B;EAC/B,MAAMI,GAAG,GAAI,2CAAb;EACA,OAAOA,GAAG,CAACF,IAAJ,CAASF,GAAT,CAAP,CAF+B,CAEV;AACxB,C,CACD;;;AACO,eAAeK,aAAf,CAA6BC,KAA7B,EAAoCC,OAApC,EAA6CC,KAA7C,EAAoD;EACvD;AACJ;AACA;AACA;AACA;EACI,MAAMC,MAAM,GAAG,MAAMC,GAAG,CAACC,EAAJ,EAArB;EACA,MAAM;IAAEC;EAAF,IAAWH,MAAjB;EACA,MAAM;IAAEI;EAAF,IAAWD,IAAjB;;EACA,IAAIJ,KAAK,IAAI,MAAb,EAAqB;IACjB,OAAOM,MAAM,CAACR,KAAD,CAAN,GAAgBQ,MAAM,CAACP,OAAD,CAA7B;EACH,CAFD,MAEO;IACH,OAAOO,MAAM,CAACR,KAAD,CAAN,GAAgBQ,MAAM,CAACP,OAAD,CAAtB,GAAkCM,IAAI,CAACL,KAAD,CAAJ,CAAYO,KAArD;EACH;AACJ;;AAAA,C,CACD;;AACO,eAAeC,UAAf,CAA0BV,KAA1B,EAAiCW,KAAjC,EAAwCC,KAAxC,EAA+C;EAClD;AACJ;AACA;AACA;AACA;AACA;AACA;EACI,IAAIH,KAAK,GAAG,EAAZ;;EACA,KAAK,IAAII,CAAT,IAAcF,KAAd,EAAqB;IACjBF,KAAK,CAACK,IAAN,CACI,MAAMf,aAAa,CACfC,KADe,EAEfY,KAAK,IAAI,CAAT,GAAaD,KAAK,CAACE,CAAD,CAAL,CAASE,SAAtB,GAAkCJ,KAAK,CAACE,CAAD,CAFxB,EAGfD,KAAK,IAAI,CAAT,GAAaD,KAAK,CAACE,CAAD,CAAL,CAASG,IAAtB,GAA6BH,CAHd,CADvB;EAOH;;EAAA;EACD,OAAOJ,KAAP;AACH;;AAAA,C,CACD;;AACO,eAAeQ,aAAf,CAA6BjB,KAA7B,EAAmCC,OAAnC,EAA4CC,KAA5C,EAAmD;EACtD,IAAIO,KAA