v-for=\"(item, index) in wayList\" :key=\"index\" clickable @click=\"onPay(item)\">\n                    <template #title>\n                        <div class=\"cell-title\">\n                            <p>{{item.name}}</p>\n                            <p>{{item.label}}</p>\n                        </div>\n                    </template>\n                    <template #right-icon>\n                        <van-icon name=\"success\" v-if=\"nowWay == item.name\" />\n                    </template>\n                </van-cell>\n        </van-cell-group>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapState } from 'vuex'\nexport default {\n    data(){\n        return{\n            nowWay:'',\n            wayList:[\n                {\n                    name:'USD',\n                    label:'$'\n                },\n                {\n                    name:'CNY',\n                    label:'¥'\n                },\n                {\n                    name:'JPY',\n                    label:'¥'\n                },\n                {\n                    name:'KRW',\n                    label:'₩'\n                },\n                {\n                    name:'INR',\n                    label:'₹'\n                },\n\n            ]\n        }\n    },  \n    components:{\n        Navigation:resolve => require(['../../../components/nav/Navigation'],resolve)\n    },\n    computed:{\n        ...mapState(['current'])\n    },\n    created(){\n        this.nowWay = this.current.amount_way;\n    },\n    methods:{\n        onPay(_item){\n            this.nowWay = _item.name;\n            this.$store.commit('current/upAmountWay',_item.name);\n        }\n    }\n\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.chose-amount-way{\n    width: 100%;\n    height: 100vh;\n    background: #F2F2F2;\n    .way-box{\n        margin-top: 6px;\n    }\n}\n.cell-title{\n    display: flex;\n    p{\n        font-size: 14px;\n        color: #333333;\n    }\n    p:first-child{\n        width: 48px;\n    }\n}\n.van-icon-success {\n    color: #2cbc94;\n    font-size: 20px;\n}\n\n</style>"]}]}