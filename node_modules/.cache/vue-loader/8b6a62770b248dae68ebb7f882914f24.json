lass=\"current-icon\">\n          <p class=\"iconfont icon-mima-3\" type=\"icon\"></p>\n          <p>交易密码</p>\n          <p>\n            去设置\n            <span class=\"iconfont icon-a-bianzu3\" type=\"icon\"></span>\n          </p>\n        </div>\n      </div>\n    </P-popup>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: {\n    value: {\n      type: Boolean,\n      default: false,\n    },\n  },\n  data() {\n    return {\n      accordingTo: false,\n    };\n  },\n  watch: {\n    value: {\n      immediate: true,\n      handler: function (v) {\n        this.accordingTo = v;\n      },\n    },\n    accordingTo(v) {\n      this.$emit(\"input\", v);\n    },\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n.auth-title {\n  display: flex;\n  align-items: center;\n  p:first-child {\n    font-size: 16px;\n    color: #333;\n  }\n  p[type=\"icon\"] {\n    font-size: 14px;\n    color: #09bb07;\n    margin-left: 4px;\n  }\n}\n.title-remark {\n  font-size: 12px;\n  color: #999;\n}\n.label-line {\n  width: 100%;\n  height: 1px;\n  background: #eee;\n  margin: 10px 0 16px 0;\n}\n.auth-step {\n  display: flex;\n  justify-content: space-around;\n  align-items: center;\n  div{\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n  }\n  .current-icon{\n    p:first-child{\n        font-size: 34px;\n        color: #333;\n    }\n    p:nth-child(2){\n        font-size: 14px;\n        color: #333;\n        margin: 3px 0 8px 0;\n    }\n    p:last-child{\n        font-size: 12px;\n        span{\n            font-size: 12px;\n        }\n    }\n  }\n  .only-icon{\n    .iconfont{\n        font-size: 22px;\n    }\n  }\n}\n</style>"]}]}