"order\" @click=\"$router.push(`/pleasepayment/${id}`)\">\n        <div class=\"head ui-flex-center ui-hx\">\n            <div class=\"left ui-flex-row\">\n                <div class=\"h4\">购买</div>\n                <div>USDT</div>\n            </div>\n            <div class=\"right ui-flex-row\">\n                <div class=\"time\">还有<span>13:44</span>结束</div>\n                <div class=\"state\">请付款</div>\n                <van-icon name=\"arrow\" />\n            </div>\n        </div>\n        <div class=\"body ui-flex-center\">\n            <div>\n                <div><span class=\"label\">单价</span>₹3.21</div>\n                <div><span class=\"label\">数量</span>1022.25 USDT</div>\n            </div>\n            <div class=\"right\">\n                <div>总金额</div>\n                <div class=\"price\" :style=\"{'color':color}\">\n                    <span class=\"symbol m-5-r\">₹</span>{{Price}}\n                </div>\n            </div>\n        </div>\n        <div class=\"footer ui-flex-center\">\n            <ul class=\"left\">\n                <li>\n                    <van-image round width=\"20\" height=\"20\" lazy-load src=\"https://img01.yzcdn.cn/vant/cat.jpeg\" />\n                </li>\n                <li class=\"name\">{{ name }}</li>\n                <li>\n                    <WeiXinXi v-model=\"badge\"></WeiXinXi>\n                </li>\n            </ul>\n            <div class=\"right ui-flex-row\">\n                {{date}}\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport WeiXinXi from '@/components/WeiXinXi'\n\nexport default {\n    components: {\n        WeiXinXi\n    },\n    props: {\n        // 订单ID\n        id:{\n            type:Number,\n            default:0\n        },  \n        //按钮名称\n        btnName: {\n            type: String,\n            default: '购买'\n        },\n        //按钮背景色和价格文字颜色\n        color: {\n            type: String,\n            default: '#2cbc94'\n        },\n        name: {\n            type: String,\n            default: ''\n        },\n        Price: {\n            type: [Number, String],\n            default: 0\n        },\n        badge: {\n            type: Number,\n            default: 0\n        },\n        date: {\n            type: String,\n            default: ''\n        }\n    },\n    // created(){\n    //     console.log(this.id)\n    // },\n};\n</script>\n\n<style lang=\"less\" scoped>\n.order {\n    .head {\n        &.ui-hx {\n            &::after {\n                background: #eeeeee;\n            }\n        }\n        .left {\n            font-weight: bold;\n            .h4 {\n                padding-left: 0px;\n                padding-right: 5px;\n                font-size: 14px;\n                color: #333333;\n                font-weight: bold;\n                &::before {\n                    width: 3px;\n                    height: 10px;\n                    margin-top: 0px;\n                }\n            }\n        }\n        .right {\n            font-size: 12px;\n            color: #999999;\n            .time {\n                span {\n                    font-size: 14px;\n                    font-weight: 600;\n                    color: #fd5656;\n                    padding: 0px 5px;\n                }\n            }\n            .state {\n                font-size: 12px;\n                font-weight: 400;\n                color: #333333;\n                padding: 0px 5px;\n            }\n            .van-icon-arrow {\n                width: 10px;\n                height: 10px;\n                color: #999999;\n            }\n        }\n    }\n\n    .body {\n        color: #666666;\n        line-height: 25px;\n        margin-top: 10px;\n\n        .right {\n            text-align: right;\n        }\n\n        .label {\n            font-size: 12px;\n            color: #999999;\n            margin-right: 5px;\n        }\n\n        .price {\n            font-size: 22px;\n            font-weight: bold;\n            .symbol {\n                font-size: 14px;\n            }\n        }\n    }\n\n    .footer {\n        color: #666666;\n        .left {\n            display: flex;\n            li {\n                height: 35px;\n                display: flex;\n                align-items: center;\n                &.name {\n                    font-size: 14px;\n                    font-weight: bold;\n                    color: #333330;\n                    margin: 0px 9px;\n                }\n                .renzhengxinxi {\n                    width: 12px;\n                    height: 13px;\n                }\n            }\n        }\n        .right {\n            height: 14px;\n            font-size: 10px;\n            font-weight: 400;\n            color: #999999;\n        }\n    }\n}\n</style>"]}]}